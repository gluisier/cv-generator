{% extends 'base.html.twig' %}

{% block title %}Experience index{% endblock %}

{% block body %}
	<h1>Experience index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>StartDate</th>
				<th>EndDate</th>
				<th>What</th>
				<th>Id</th>
				<th>DeletedAt</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
		{% for experience in experiences %}
			<tr>
				<td>{{ experience.startDate ? experience.startDate|date('Y-m-d') : '' }}</td>
				<td>{{ experience.endDate ? experience.endDate|date('Y-m-d') : '' }}</td>
				<td>{{ experience.what }}</td>
				<td>{{ experience.id }}</td>
				<td>{{ experience.deletedAt ? experience.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
				<td>
					<a href="{{ path('app_experience_show', {'id': experience.id}) }}">show</a>
					<a href="{{ path('app_experience_edit', {'id': experience.id}) }}">edit</a>
				</td>
			</tr>
		{% else %}
			<tr>
				<td colspan="6">no records found</td>
			</tr>
		{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_experience_new') }}">Create new</a>
{% endblock %}
