{% extends 'base.html.twig' %}

{% block title %}Company{% endblock %}

{% block body %}
	<h1>Company</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ company.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ company.name }}</td>
			</tr>
			<tr>
				<th>Link</th>
				<td>{{ company.link }}</td>
			</tr>
			<tr>
				<th>Address.street</th>
				<td>{{ company.address.street }}</td>
			</tr>
			<tr>
				<th>Address.postalCode</th>
				<td>{{ company.address.postalCode }}</td>
			</tr>
			<tr>
				<th>Address.city</th>
				<td>{{ company.address.city }}</td>
			</tr>
			<tr>
				<th>Address.country</th>
				<td>{{ company.address.country }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_company_index') }}">back to list</a>

	<a href="{{ path('app_company_edit', {'id': company.id}) }}">edit</a>

	{{ include('company/_delete_form.html.twig') }}
{% endblock %}
