<style title="Design 2023">
<!--/*--><![CDATA[/*><!--*/
body {
	--page-width: 1240px;
	--image-width: 435px;
	--middle-column-width: 280px;
	--space: 1.5em;
	--half-space: 0.75em;
	--dark: #3B3A3B;
	--light: #EAEBEC;
	--blue: #56C8EA;
	font-family: "Open Sans", Arial, Verdana, Helvetica, sans-serif;
	margin: 0;
	padding: 0;
}
h1 {
	margin: 0;
	font-size: 92px;
	line-height: 1em;
	width: 200px;
	letter-spacing: .1em;
}
h2 {
	color: var(--blue);
	font-variant: small-caps;
}
h1, h2, h3, h4 {
	font-family: Jost, Arial, Verdana, Helvetica, sans-serif;
}
blockquote {
	margin: 0;
	text-align: justify;
	font-size: large;
	line-height: 1.75em;
}
div.vcard.h-card {
	width: var(--page-width);
	height: var(--page-height);
	margin: auto;
	display: grid;
	grid-template:
		"image  lead"
		"aside  main"
		"footer main" / var(--image-width);
	background-color: var(--dark);
}
img {
	grid-area: image;
	width: var(--image-width);
	aspect-ratio: 1/1;
	filter: grayscale(1);
	display: flex;
	justify-content: space-evenly;
	align-items: center;
}
#Lead {
	grid-area: lead;
	padding: 1.5em;
}
#Lead h2 {
	font-size: 40px;
}
img, #Lead {
	color: var(--dark);
	background-color: var(--light);
}
aside, footer {
	padding-left: var(--space);
	width: var(--image-width);
	grid-area: aside;
	box-sizing: border-box;
	color: white;
}
p {
	margin: 0;
}
.h2 {
	display: flex;
	align-items: center;
}
.h2 h2 {
	padding: 0 .5rem;
	font-weight: normal;
}
.spacer {
	flex-basis: 0;
	flex-grow: 1;
	border-bottom: 0.15em solid white;
}
aside>p:first-of-type {
	text-align: center;
	margin-top: 0.175em;
}
address {
	font-style: normal;
}
aside>section>*:not(.h2) {
	margin-right: var(--space);
}
#Contact > address + p, #Contact > p + p {
	margin-top: var(--half-space);
}
#Contact i {
	margin-right: 1em;
	color: var(--blue);
}
a {
	color: var(--blue);
}
form {
	background-color: #ffffff0a;
}
ul {
	margin: 0;
}
input[type="search"] {
	width: 100%;
	border: 1px solid var(--blue);
}
#ComputerScience ul {
	padding: 0;
}
#ComputerScience li {
	list-style: none;
}
#ComputerScience li:not([hidden]) {
	display: inline-block;
}
#ComputerScience li>input {
	margin: 0 0 0 0.5em;
}
#ComputerScience li>label {
	padding: 0 0.5em;
}
#Languages>div[id] {
	display: flex;
	flex-wrap: wrap;
}
#Languages>div[id] + div[id] {
	margin-top: var(--half-space);
}
#Languages>div[id] h3 + p {
	margin-left: auto;
	align-self: center;
}
#Languages>div[id] h3 + p + p {
	text-align: justify;
	font-size: small;
}
#Hobbies>[id] + div[id] {
	margin-top: var(--half-space);
}
main {
	grid-area: main;
	padding: 0 var(--space) var(--space);
	box-sizing: border-box;
	background-color: white;
	color: var(--dark);
}
main h2 {
	padding-bottom: calc(var(--half-space) / 3);
	border-bottom: 0.1em solid transparent;
	border-image: linear-gradient(to right, transparent, transparent var(--space), var(--blue) var(--space)) 1;
}
.p-experience {
	display: grid;
	grid-template:
		"organizer    role"
		"period       summaries"
		"titles       summaries"
		"realisations realisations" / var(--middle-column-width);
	column-gap: 1em;
}
.p-experience + .p-experience {
	margin-top: var(--half-space);
}
abbr {
	display: inline !important;
}
[itemprop="organizer"] {
	grid-area: organizer;
	align-self: first baseline;
}
[itemprop="role"] {
	grid-area: role;
	align-content: first baseline;
}
.time {
	grid-area: period;
	font-size: small;
}
.summaries {
	grid-area: summaries;
	display: flex;
	flex-direction: column;
	font-size: small;
}
h4 {
	margin: 0;
}
.titles {
	grid-area: titles;
	align-self: end;
}
.realisations {
	grid-area: realisations;
}
details>details {
	margin-left: var(--half-space);
}
.p-achievement ul {
	padding-left: 1em;
}
h4 {
	font-weight: normal;
	font-style: italic;
}
h5 {
	display: inline-block;
	font-weight: normal;
	font-size: small;
}
h3, h4, h5 {
	margin: 0;
}
summary.p-summary + * {
	font-size: small;
	border: 2px solid #0000;
	border-image: radial-gradient(at 50% 0, white 70%, var(--blue)) 10;
}
.technologies {
	font-size: x-small;
	font-style: italic;
}
#Training .vevent, #References>div {
	display: grid;
	grid-template-columns: calc(var(--image-width) / 2) auto;
}
#Training .vevent>div>p {
	font-size: small;
}
#Training .vevent + .vevent, #References>div + div {
	margin-top: var(--half-space);
}
footer {
	grid-area: footer;
	align-self: end;
	padding-bottom: var(--space);
	font-size: small;
}
@media screen {
input[type="search"] {
	animation: zoom 1s 2 1s alternate;
	transform-origin: bottom left;
}
@keyframes zoom {
	to {
		transform: scale(2);
	}
}
.noscreen {
	display: none !important;
}
#ComputerScience li:not(:first-of-type) + li {
	border-left: 1px solid;
}
details>::marker {
	font: var(--fa-font-solid);
	color: var(--blue);
	content: '\f06e ';
	font-size: small;
}
details[open]>::marker {
	font: var(--fa-font-solid);
	content: '\f070 ';
	font-size: small;
}
abbr[title] {
	text-decoration: dotted underline;
}
}
@media print {
@page {
	size: 1240px  1754px; /* @150 DPI */
	margin: 1.5em 0 1.5em 0 !important;
}
@page:first {
	margin-top: 0 !important;
}
body {
	margin: 0 !important;
	padding: 0 !important;
}
img, #Lead {
	background-color: white;
}
div.vcard.h-card {
	page-break-inside: avoid;
	background-color: white;
}
h1, h2, h3, h4, h5 {
	page-break-after: avoid;
}
img, #Lead {
	page-break-after: avoid;
}
aside, footer {
	color: var(--dark);
}
aside, main {
	page-break-before: avoid;
	page-break-inside: auto;
}
.spacer {
	border-color: var(--dark);
}
#ComputerScience li:not(:first-of-type) + li:not(.noprint) label {
	border-left: 1px solid;
}
section {
	page-break-inside: auto;
}
section > div {
	page-break-inside: avoid;
	page-break-before: auto;
	page-break-after: auto;
}
summary {
	page-break-after: avoid;
}
summary summary+* {
	page-break-inside: auto;
}
.noprint {
	display: none !important;
}
}
/*]]>*/-->
</style>